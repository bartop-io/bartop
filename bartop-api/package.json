{
  "name": "bartop-api",
  "version": "0.0.1",
  "description": "Server for running BarTop's API.",
  "main": "server.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "database": "rethinkdb",
    "test": "NODE_ENV=test nyc --all mocha test/**/*.js",
    "unit-tests": "NODE_ENV=test mocha test/unit/**/*.js",
    "integration-tests": "NODE_ENV=test mocha test/integration/**/*.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint src test",
    "fix": "eslint --fix src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpopp07/bartop.git"
  },
  "keywords": [
    "bartop"
  ],
  "author": "Dustin Popp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpopp07/bartop/issues"
  },
  "homepage": "https://github.com/dpopp07/bartop#readme",
  "dependencies": {
    "ajv": "^6.5.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-jwt": "^5.3.1",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "helmet": "^3.12.1",
    "jwks-rsa": "^1.3.0",
    "lodash.merge": "^4.6.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "rethinkdbdash": "^2.3.31",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.5",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "mocha-prepare": "^0.1.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7",
    "supertest": "^3.1.0"
  },
  "nyc": {
    "include": [
      "**/src/**/*.js"
    ],
    "exclude": [
      "**/src/utils/seed.js"
    ]
  }
}
