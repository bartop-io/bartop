{
  "name": "bartop-ui",
  "version": "0.0.1",
  "description": "Web app for BarTop's UI.",
  "dependencies": {
    "auth0-js": "^9.4.2",
    "formik": "^0.11.11",
    "jwt-decode": "^2.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "serialize-error": "^2.1.0",
    "styled-components": "^3.2.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom --coverage --silent",
    "watch-test": "react-app-rewired test --env=jsdom --silent",
    "watch-test-with-logs": "react-app-rewired test --env=jsdom",
    "coverage": "codecov",
    "lint": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "fix": "eslint --fix src",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "proxy": "http://localhost:3001",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpopp07/bartop.git"
  },
  "keywords": [
    "bartop"
  ],
  "author": "James Clements",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpopp07/bartop/issues"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.3.3",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "jest-styled-components": "^5.0.0",
    "prettier": "^1.11.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.5.0",
    "redux-mock-store": "^1.5.1",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "@storybook/react": "^3.4.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/test-helpers/",
      "!src/stories/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
