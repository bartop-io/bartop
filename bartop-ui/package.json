{
  "name": "bartop-ui",
  "version": "0.0.1",
  "description": "Web app for BarTop's UI.",
  "dependencies": {
    "auth0-js": "^9.4.1",
    "formik": "^0.11.11",
    "jwt-decode": "^2.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "serialize-error": "^2.1.0",
    "styled-components": "^2.4.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom --coverage --silent",
    "watch-test": "react-app-rewired test --env=jsdom --silent",
    "watch-test-with-logs": "react-app-rewired test --env=jsdom",
    "coverage": "codecov",
    "lint": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "fix": "eslint --fix src",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3001",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpopp07/bartop.git"
  },
  "keywords": [
    "bartop"
  ],
  "author": "James Clements",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpopp07/bartop/issues"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.7.0",
    "jest-styled-components": "^4.9.0",
    "prettier": "^1.7.4",
    "react-app-rewire-styled-components": "^3.0.0",
    "react-app-rewired": "^1.3.5",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/test-helpers/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
