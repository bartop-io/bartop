{
  "name": "bartop-ui",
  "version": "0.0.1",
  "description": "Web app for BarTop's UI.",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "styled-components": "^2.2.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom --coverage",
    "watch-test": "react-app-rewired test --env=jsdom",
    "coverage": "codecov",
    "lint": "eslint src",
    "fix": "eslint --fix src",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3001",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpopp07/bartop.git"
  },
  "keywords": [
    "bartop"
  ],
  "author": "James Clements",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpopp07/bartop/issues"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest-styled-components": "^4.9.0",
    "prettier": "^1.7.4",
    "react-app-rewire-styled-components": "^3.0.0",
    "react-app-rewired": "^1.3.5",
    "react-test-renderer": "^16.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
